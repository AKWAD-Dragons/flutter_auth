{
  "dart.flutterSdkPath": ".fvm/flutter_sdk",
  // "dart.dartSdkPath": "."
  // Remove .fvm files from search
  "search.exclude": {
    "**/.fvm": true
  },
  // Remove from file watching
  "files.watcherExclude": {
    "**/.fvm": true
  },
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flutter Flavor dev",
      "request": "launch",
      "type": "dart",
      "args": ["--flavor", "dev"]
    }
  ]
}
